{
  "name": "gotify-ext",
  "version": "0.3.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod && cp ./src/manifest.prod.json ./dist/gotify-ext/manifest.json && sed -i -e \"s/{|vers|}/$(git describe --tags --abbrev=0 | cut -c 2-)/g\" ./dist/gotify-ext/manifest.json && cd ./dist/gotify-ext && zip -r gotify-ext-dist.zip ./*",
    "deploy": "npm run prod && ./deploy.fish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "src-zip": "zip -r gotify-ext-src ./* -x 'node_modules/*' '.git/*' 'gotify_dev/gotify/*' 'dist/*' 'deploy.fish'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "ngx-scrollbar": "^7.2.1-beta.0",
    "ngx-toastr": "^12.0.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~11.1.0",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.0",
    "@types/chrome": "0.0.112",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.1",
    "chrome-webstore-upload-cli": "^1.2.0",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  }
}
