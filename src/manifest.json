{
  "name": "Gotify",
  "version": "1.0",
  "description": "Unofficial browser extension for Gotify",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js",
      "runtime.js",
      "vendor.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}
