{
  "name": "Gotify",
  "version": "{|vers|}",
  "description": "Unofficial browser extension for Gotify",
  "manifest_version": 2,
  "icons": {
    "16": "assets/logo-16.png",
    "48": "assets/logo-48.png",
    "128": "assets/logo-128.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ],
    "styles": [
      "styles.css"
    ]
  },
  "permissions": [
    "http://*/",
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}
